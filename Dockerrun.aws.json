{
   "AWSEBDockerrunVersion":2,
   "volumes":[
      {
         "name":"mongodata",
         "host":{
            "sourcePath":"/var/app/current/mongo/data/"
         }
      }
   ],
   "containerDefinitions":[
      {
         "name":"zuul-api-gateway",
         "image":"zohancs/stock-market-api-gateway:1.0.1",
         "essential":false,
         "memory":500,
         "portMappings":[
            {
               "hostPort":7000,
               "containerPort":7000
            }
         ],
         "links":[
            "eureka-naming-server"
         ],
         "environment":[
            {
               "name":"EUREKA_URI",
               "value":"http://eureka-naming-server:8761/eureka"
            }
         ],
         "mountPoints":[
            
         ]
      },
      {
         "name":"eureka-naming-server",
         "image":"zohancs/stock-market-eureka-server:1.0.0",
         "environment":[
            
         ],
         "essential":false,
         "memory":500,
         "mountPoints":[
            
         ],
         "portMappings":[
            {
               "hostPort":8761,
               "containerPort":8761
            }
         ]
      },
      {
         "name":"manage-company-service",
         "image":"zohancs/stock-market-company-service:1.0.1",
         "essential":false,
         "memory":500,
         "portMappings":[
            {
               "hostPort":8000,
               "containerPort":8000
            }
         ],
         "links":[
            "eureka-naming-server"
         ],
         "environment":[
            {
               "name":"EUREKA_URI",
               "value":"http://eureka-naming-server:8761/eureka"
            },
            {
               "name":"MYSQL_HOST",
               "value":"stock-market-sql-db.cbhfbuvxulgp.us-east-2.rds.amazonaws.com"
            },
            {
               "name":"MYSQL_USER",
               "value":"root"
            },
            {
               "name":"MYSQL_PASSWORD",
               "value":"Stock0709!"
            }
         ],
         "mountPoints":[
            
         ]
      },
      {
         "name":"stock-service",
         "image":"zohancs/stock-market-stock-service:1.0.1",
         "essential":false,
         "memory":500,
         "portMappings":[
            {
               "hostPort":9000,
               "containerPort":9000
            }
         ],
         "mountPoints":[
            
         ],
         "links":[
            "eureka-naming-server",
            "mongodb"
         ],
         "environment":[
            {
               "name":"EUREKA_URI",
               "value":"http://eureka-naming-server:8761/eureka"
            },
            {
               "name":"MONGO_HOST",
               "value":"mongodb"
            }
         ]
      },
      {
         "name":"mongodb",
         "image":"mongo",
         "essential":true,
         "memory":500,
         "portMappings":[
            {
               "hostPort":27017,
               "containerPort":27017
            }
         ],
         "mountPoints":[
            {
               "sourceVolume":"mongodata",
               "containerPath":"/data/db"
            }
         ]
      }
   ]
}